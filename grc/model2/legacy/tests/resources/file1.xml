<?xml version="1.0"?>
<block>
    <name>testname</name>
    <key>block_key</key>
    <import>from gnuradio import blocks</import>
    <make>blocks.complex_to_mag_squared($vlen)</make>
    <param>
        <name>Vec Length</name>
        <key>vlen</key>
        <value>1</value>
        <type>int</type>
    </param>
    <check>$vlen &gt; 0</check>
    <sink>
        <name>in</name>
        <type>complex</type>
        <vlen>$vlen</vlen>
    </sink>
    <source>
        <name>out</name>
        <type>float</type>
        <vlen>$vlen</vlen>
    </source>
</block>
